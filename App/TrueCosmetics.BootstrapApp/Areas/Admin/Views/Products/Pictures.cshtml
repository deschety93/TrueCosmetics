@model IEnumerable<string>
@{
    ViewBag.Title = "Pictures";
    Layout = null;
}

<div class="row">
    @foreach (var path in Model)
        {
        <div class="col-lg-3 col-md-4 col-xs-6 thumb">
            <a class="thumbnail" href="#">
                @Html.Image(Url.Content(path), new { @class = "img-responsive", width = 150})
            </a>
        </div>
    }
</div>